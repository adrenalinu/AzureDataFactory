{
	"name": "MainPipeline",
	"properties": {
		"activities": [
			{
				"name": "Copy data from Cloud SQL to DWH",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "ClearDWH",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT 'Cloud' as Source, [OrderID], Sum([OrderAmount]) as OrderTotal\nFROM [dbo].[Orders] GROUP BY [OrderID]",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "SqlDWSink"
					},
					"enableStaging": false,
					"parallelCopies": 4,
					"dataIntegrationUnits": 4,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Source",
									"type": "String"
								},
								"sink": {
									"name": "SystemSource",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "OrderID",
									"type": "Int32"
								},
								"sink": {
									"name": "SalesOrderID",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "OrderTotal",
									"type": "Decimal"
								},
								"sink": {
									"name": "OrderTotal",
									"type": "Decimal"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "AzureSqlDatabase",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSynapseAnalytics",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "ClearDWH",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "dbo.sp_TruncateSalesOrders"
				},
				"linkedServiceName": {
					"referenceName": "AzureSynapseAnalytics1",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Wait",
				"type": "Wait",
				"dependsOn": [
					{
						"activity": "Copy data from Cloud SQL to DWH",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"waitTimeInSeconds": 1
				}
			},
			{
				"name": "ExportToJSON",
				"type": "ExecuteDataFlow",
				"dependsOn": [
					{
						"activity": "Wait",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "1.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "ExportToJSON",
